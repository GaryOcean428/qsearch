{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "RAILPACK"
  },
  "deploy": {
    "startCommand": "python -m gunicorn qsearch.api.main:app -k uvicorn.workers.UvicornWorker --bind [::]:$PORT --workers 2 --threads 4",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
